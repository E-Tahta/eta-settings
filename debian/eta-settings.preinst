#!/bin/bash

set -e

if [ -f /etc/kde4/kdm/Xreset ]; then
    mv /etc/kde4/kdm/Xreset /etc/kde4/kdm/Xreset.orig
fi

if [ -f /etc/kde4/kdm/kdmrc ]; then
